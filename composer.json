{
    "name": "bretrzaun/deployment-command",
    "description": "Console-Command to deploy an application to (multiple) servers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bret R. Zaun",
            "email": "bretrzaun@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "laravel/envoy": "^1.2||^2.0",
        "symfony/console": "^4.0||^5.0||^6.0",
        "symfony/options-resolver": "^4.0||^5.0||^6.0",
        "symfony/process": "^4.0||^5.0||^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BretRZaun\\DeploymentCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BretRZaun\\DeploymentCommand\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/deploy"
    ],
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
