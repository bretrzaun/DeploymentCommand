{
    "name": "bretrzaun/deployment-command",
    "description": "Console-Command to deploy an application to (multiple) servers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bret R. Zaun",
            "email": "bretrzaun@hotmail.com"
        }
    ],
    "require": {
        "symfony/console": "^3.0|^4.0",
        "laravel/envoy" : "^1.2"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^6.0",
        "symfony/var-dumper" : "^3.3"
    },
    "autoload" : {
        "psr-4" : {
            "BretRZaun\\DeploymentCommand\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "BretRZaun\\DeploymentCommand\\Tests\\" : "tests/"
        }
    },
    "config" : {
        "platform" : {
            "php" : "7.0"
        }
    },
    "bin": [
        "bin/deploy"
    ]
}
